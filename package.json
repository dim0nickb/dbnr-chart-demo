{
  "name": "dbnr-chart-demo",
  "version": "1.0.1",
  "description": "dbnr-chart-demo",
  "scripts": {
    "ngc-aot": "rimraf tmp/aot && node --max_old_space_size=4096 node_modules/@angular/compiler-cli/src/main.js -p tsconfig-aot.json --locale=en-US",
    "bundle-aot": "rimraf dist/aot && node --max_old_space_size=8192 node_modules/rollup/bin/rollup -c rollup-config-aot.js",
    "copy-aot": "node tools/copy-aot.js",
    "build-aot": "npm run ngc-aot && npm run bundle-aot && npm run copy-aot",
    "tsc-jit": "tsc -p tsconfig.json",
    "tsc-jit-w": "tsc -w -p tsconfig.json",
    "serve": "lite-server -c=bs-config.json",
    "start": "concurrently \"npm run tsc-jit-w\" \"npm run serve\"",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.2.6",
    "@angular/common": "^4.2.6",
    "@angular/compiler": "^4.2.6",
    "@angular/compiler-cli": "^4.2.6",
    "@angular/core": "^4.2.6",
    "@angular/forms": "^4.2.6",
    "@angular/http": "^4.2.6",
    "@angular/platform-browser": "^4.2.6",
    "@angular/platform-browser-dynamic": "^4.2.6",
    "@angular/platform-server": "^4.2.6",
    "@angular/router": "^4.2.6",
    "@types/jquery": "^3.2.15",
    "at-grid": "^1.0.11",
    "commander": "^2.11.0",
    "core-js": "^2.5.1",
    "dbnr-chart": "^1.0.2",
    "debug": "^2.6.8",
    "jquery": "^3.2.1",
    "rxjs": "~5.4.3",
    "spawn-command": "0.0.2",
    "systemjs": "^0.20.19",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.53",
    "@types/node": "^8.0.11",
    "async": "^2.5.0",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "copy-dir": "^0.3.0",
    "del": "^3.0.0",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "eslint": "^4.9.0",
    "eslint-config-google": "^0.9.1",
    "tslint": "^5.5.0",
    "typescript": "~2.3.3"
  },
  "repository": {}
}
